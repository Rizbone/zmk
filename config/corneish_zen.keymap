		};



		// -----------------------------------------------------|
		// Layer 1 - mNR = 	  Zahlen 						|
		// -----------------------------------------------------|

		mac_zahlen
		{
			label = "MAC";

			bindings = <

				// ----------------------------------------

				&kp ESC	
