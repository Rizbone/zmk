		// -----------------------------------------------------|
		// Layer 10 - mSYM =	Symoble							|
		// -----------------------------------------------------|

		mac_symbole
		{
			label = "MAC";

			bindings = <

				// ----------------------------------------
				
				&kp LG(LC(SPACE))   	// @Sonderzeichen
				&kp LS(RBRC)			// *
				&kp LS(N3)				// §
				&kp LS(N2)				// "
				&kp LA(L)				// @
